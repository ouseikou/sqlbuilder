{
  "swagger": "2.0",
  "info": {
    "title": "proto/api.proto",
    "version": "version not set"
  },
  "tags": [
    {
      "name": "SqlBuilderApi"
    }
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {},
  "definitions": {
    "protoBasicData": {
      "type": "object",
      "properties": {
        "int_val": {
          "type": "string",
          "format": "int64",
          "title": "覆盖 int32"
        },
        "double_val": {
          "type": "number",
          "format": "double",
          "title": "覆盖 float"
        },
        "str_val": {
          "type": "string",
          "title": "字符串会默认加单引号"
        },
        "logic": {
          "type": "boolean"
        },
        "str_literal": {
          "$ref": "#/definitions/protoStringLiteral",
          "title": "字符串字面量不会默认加单引号"
        }
      }
    },
    "protoBasicDataArr": {
      "type": "object",
      "properties": {
        "args": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protoBasicData"
          }
        }
      }
    },
    "protoBuilderStrategy": {
      "type": "string",
      "enum": [
        "BUILDER_STRATEGY_UNSPECIFIED",
        "BUILDER_STRATEGY_MODEL",
        "BUILDER_STRATEGY_TEMPLATE"
      ],
      "default": "BUILDER_STRATEGY_UNSPECIFIED"
    },
    "protoCallType": {
      "type": "string",
      "enum": [
        "CALL_TYPE_UNSPECIFIED",
        "CALL_TYPE_AGG",
        "CALL_TYPE_INNER",
        "CALL_TYPE_CUSTOM",
        "CALL_TYPE_ARITH",
        "CALL_TYPE_LITERAL",
        "CALL_TYPE_BOOLEAN"
      ],
      "default": "CALL_TYPE_UNSPECIFIED",
      "title": "- CALL_TYPE_ARITH: e.g. + - / * %\n - CALL_TYPE_BOOLEAN: e.g.  =, !=, \u003c, \u003e, \u003c=, \u003e=, between, is null, is not null, etc"
    },
    "protoCaseWhen": {
      "type": "object",
      "properties": {
        "conditions": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protoCaseWhenItem"
          }
        },
        "else_value": {
          "$ref": "#/definitions/protoBasicData"
        },
        "alias": {
          "type": "string"
        },
        "use_as": {
          "type": "boolean"
        }
      }
    },
    "protoCaseWhenItem": {
      "type": "object",
      "properties": {
        "when": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protoMixWhere"
          }
        },
        "then": {
          "$ref": "#/definitions/protoBasicData"
        }
      },
      "title": "每一行 case-when-then就是多个条件组成的语句"
    },
    "protoColumn": {
      "type": "object",
      "properties": {
        "field": {
          "type": "string"
        },
        "table": {
          "type": "string"
        },
        "schema": {
          "type": "string"
        },
        "alias": {
          "type": "string"
        },
        "agg_able": {
          "type": "boolean"
        },
        "use_as": {
          "type": "boolean"
        }
      }
    },
    "protoCommonResponse": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "msg": {
          "type": "string"
        },
        "data": {
          "type": "object",
          "additionalProperties": {
            "type": "string"
          }
        }
      }
    },
    "protoCondition": {
      "type": "object",
      "properties": {
        "field": {
          "$ref": "#/definitions/protoMixField",
          "title": "暂时不考虑 where 1=1, where true; MixField.Expression 覆盖 WHERE ARRAY_LENGTH(my_array, 1) \u003e 5 场景"
        },
        "args": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protoBasicData"
          }
        },
        "operator": {
          "$ref": "#/definitions/protoOp"
        },
        "logic": {
          "$ref": "#/definitions/protoLogic",
          "title": "和上一个条件的逻辑关系, 默认 and, 可选or"
        },
        "reverse": {
          "type": "boolean",
          "title": "是否对该条件取反(NOT)"
        },
        "group_id": {
          "type": "string",
          "title": "组内根据logic独立构建, 组之间是and"
        },
        "use_pnt": {
          "type": "boolean",
          "title": "该条件块是否加小括号, 新增 pnt-format"
        },
        "literal_cond": {
          "$ref": "#/definitions/protoStringLiteral",
          "title": "如果是字面量，将不再执行 [MixField Op BasicData], 直接与其他条件逻辑与逻辑或"
        }
      }
    },
    "protoDeepWrapper": {
      "type": "object",
      "properties": {
        "deep": {
          "type": "integer",
          "format": "int32"
        },
        "sql": {
          "$ref": "#/definitions/protoMixSql"
        }
      }
    },
    "protoDriver": {
      "type": "string",
      "enum": [
        "DRIVER_UNSPECIFIED",
        "DRIVER_POSTGRES",
        "DRIVER_MYSQL",
        "DRIVER_DORIS"
      ],
      "default": "DRIVER_UNSPECIFIED"
    },
    "protoExpression": {
      "type": "object",
      "properties": {
        "call": {
          "type": "string"
        },
        "call_type": {
          "$ref": "#/definitions/protoCallType"
        },
        "vars": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protoMixVars"
          }
        },
        "call_as": {
          "type": "string"
        },
        "use_as": {
          "type": "boolean"
        },
        "use_pnt": {
          "type": "boolean",
          "title": "表达式是否加小括号, 新增 pnt-format"
        },
        "str_literal": {
          "$ref": "#/definitions/protoStringLiteral",
          "title": "处理极其复杂的表达式或公式, 调用方直接将字面量片段给到 sqlbuilder, 不做任何处理"
        }
      },
      "title": "表达式不在vars使用类似递归, 而是平铺可能好点. ExpressionWrapper{int32 deep = 1; repeated Expression exps = 2;}"
    },
    "protoJoin": {
      "type": "object",
      "properties": {
        "type": {
          "$ref": "#/definitions/protoJoinType"
        },
        "table": {
          "$ref": "#/definitions/protoMixTable"
        },
        "joinCond": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protoJoinCond"
          }
        }
      }
    },
    "protoJoinCond": {
      "type": "object",
      "properties": {
        "onField": {
          "$ref": "#/definitions/protoOnField"
        },
        "onCond": {
          "$ref": "#/definitions/protoCondition"
        }
      }
    },
    "protoJoinType": {
      "type": "string",
      "enum": [
        "JOIN_TYPE_UNSPECIFIED",
        "JOIN_TYPE_LEFT",
        "JOIN_TYPE_RIGHT",
        "JOIN_TYPE_INNER",
        "JOIN_TYPE_FULL",
        "JOIN_TYPE_CROSS"
      ],
      "default": "JOIN_TYPE_UNSPECIFIED"
    },
    "protoLimit": {
      "type": "object",
      "properties": {
        "limit_n": {
          "type": "integer",
          "format": "int32"
        },
        "offset": {
          "type": "integer",
          "format": "int32"
        }
      }
    },
    "protoLiteralTable": {
      "type": "object",
      "properties": {
        "sub_literal": {
          "$ref": "#/definitions/protoStringLiteral"
        },
        "literal_alias": {
          "type": "string"
        }
      }
    },
    "protoLogic": {
      "type": "string",
      "enum": [
        "LOGIC_UNSPECIFIED",
        "LOGIC_AND",
        "LOGIC_OR",
        "LOGIC_NOT"
      ],
      "default": "LOGIC_UNSPECIFIED"
    },
    "protoLogicGroup": {
      "type": "object",
      "properties": {
        "logic": {
          "$ref": "#/definitions/protoLogic",
          "title": "当前 group 的逻辑操作（AND / OR）"
        },
        "children": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protoLogicNode"
          },
          "title": "子节点（可继续是 group 或 leaf）"
        },
        "use_pnt": {
          "type": "boolean",
          "title": "是否加括号包裹整个 group"
        }
      }
    },
    "protoLogicNode": {
      "type": "object",
      "properties": {
        "group": {
          "$ref": "#/definitions/protoLogicGroup",
          "title": "一个逻辑组合（AND/OR）递归结构"
        },
        "leaf": {
          "$ref": "#/definitions/protoMixWhere",
          "title": "一个条件：Condition 或 Expression"
        }
      }
    },
    "protoMixField": {
      "type": "object",
      "properties": {
        "column": {
          "$ref": "#/definitions/protoColumn"
        },
        "expression": {
          "$ref": "#/definitions/protoExpression"
        },
        "case_when": {
          "$ref": "#/definitions/protoCaseWhen"
        }
      },
      "title": "todo  未处理: 添加 Condition 会导致递归嵌套\nSELECT array_length(array_column, 1) \u003e 0 FROM my_table; SELECT NOT column_name FROM my_table; =\u003e Condition"
    },
    "protoMixSql": {
      "type": "object",
      "properties": {
        "model": {
          "$ref": "#/definitions/protoSqlReference"
        },
        "template": {
          "$ref": "#/definitions/protoSqlText"
        }
      }
    },
    "protoMixTable": {
      "type": "object",
      "properties": {
        "normal_table": {
          "$ref": "#/definitions/protoTable"
        },
        "literal_table": {
          "$ref": "#/definitions/protoLiteralTable"
        }
      }
    },
    "protoMixVars": {
      "type": "object",
      "properties": {
        "column": {
          "$ref": "#/definitions/protoColumn"
        },
        "expression": {
          "$ref": "#/definitions/protoExpression"
        },
        "multi_condition": {
          "$ref": "#/definitions/protoMultiCondition"
        },
        "logic_condition": {
          "$ref": "#/definitions/protoLogicGroup",
          "title": "todo 暂未适配"
        },
        "context": {
          "type": "string"
        },
        "number": {
          "type": "string",
          "format": "int64"
        },
        "double_num": {
          "type": "number",
          "format": "double"
        },
        "str_literal": {
          "$ref": "#/definitions/protoStringLiteral",
          "title": "字符串字面量不会默认加单引号"
        }
      }
    },
    "protoMixWhere": {
      "type": "object",
      "properties": {
        "condition": {
          "$ref": "#/definitions/protoCondition"
        },
        "expression": {
          "$ref": "#/definitions/protoExpression",
          "title": "e.g. WHERE ARRAY_CONTAINS(tags, 'urgent') =\u003e 视作 Condition.MixField.Expression"
        }
      }
    },
    "protoMultiCondition": {
      "type": "object",
      "properties": {
        "conditions": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protoMixWhere"
          }
        }
      },
      "title": "多个条件组合"
    },
    "protoOnField": {
      "type": "object",
      "properties": {
        "left": {
          "$ref": "#/definitions/protoMixField"
        },
        "right": {
          "$ref": "#/definitions/protoMixField"
        },
        "on": {
          "$ref": "#/definitions/protoOp"
        },
        "logic": {
          "$ref": "#/definitions/protoLogic",
          "title": "和下一个条件的逻辑关系, 默认 and, 可选or"
        }
      }
    },
    "protoOp": {
      "type": "string",
      "enum": [
        "OP_UNSPECIFIED",
        "OP_EQ",
        "OP_NEQ",
        "OP_LT",
        "OP_LTE",
        "OP_GT",
        "OP_GTE",
        "OP_LIKE",
        "OP_NOT_LIKE",
        "OP_PREFIX_LIKE",
        "OP_LIKE_SUFFIX",
        "OP_BETWEEN",
        "OP_IN",
        "OP_NOT_IN",
        "OP_IS_NULL",
        "OP_IS_NOT_NULL",
        "OP_AND",
        "OP_OR"
      ],
      "default": "OP_UNSPECIFIED",
      "title": "- OP_UNSPECIFIED: 默认值或未定义状态\n - OP_EQ: \"=\"\n - OP_NEQ: \"\u003c\u003e\"\n - OP_LT: \"\u003c\"\n - OP_LTE: \"\u003c=\"\n - OP_GT: \"\u003e\"\n - OP_GTE: \"\u003e=\"\n - OP_LIKE: \"like\"\n - OP_NOT_LIKE: \"not like\", like \u0026\u0026 reverse=true\n - OP_PREFIX_LIKE: \"left like = %s模糊\"\n - OP_LIKE_SUFFIX: like right = 模糊%s\"\n - OP_BETWEEN: \"between\"\n - OP_IN: \"in\"\n - OP_NOT_IN: \"not in\"\n - OP_IS_NULL: \"is null\"\n - OP_IS_NOT_NULL: \"is not null\"\n - OP_AND: \"or\"\n - OP_OR: \"and\""
    },
    "protoOrderBy": {
      "type": "object",
      "properties": {
        "dependent": {
          "$ref": "#/definitions/protoMixField"
        },
        "order": {
          "type": "string"
        }
      }
    },
    "protoSqlReference": {
      "type": "object",
      "properties": {
        "from": {
          "$ref": "#/definitions/protoMixTable"
        },
        "join": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protoJoin"
          }
        },
        "where": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protoMixWhere"
          }
        },
        "group_by": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protoMixField"
          },
          "title": "GroupBy 可以是 Column 或 Expression"
        },
        "aggregation": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protoExpression"
          }
        },
        "select": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protoMixField"
          },
          "title": "Select 可以是 Column 或 Expression"
        },
        "order_by": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protoOrderBy"
          }
        },
        "limit": {
          "$ref": "#/definitions/protoLimit"
        },
        "logic_where": {
          "$ref": "#/definitions/protoLogicGroup",
          "title": "新结构(where 递归树表达), 优先级\u003eMixWhere(2)\n不适用于过往 where-cond 条件之间逻辑处理, 要求明确指定逻辑分组, 不同逻辑只能是不同分组\n因此需要单独查询一次构造 where-group-cond"
        }
      }
    },
    "protoSqlText": {
      "type": "object",
      "properties": {
        "text": {
          "type": "string"
        },
        "args": {
          "type": "object",
          "additionalProperties": {
            "$ref": "#/definitions/protoTemplateArg"
          }
        }
      }
    },
    "protoStringLiteral": {
      "type": "object",
      "properties": {
        "literal": {
          "type": "string"
        }
      },
      "title": "字符串字面量"
    },
    "protoTable": {
      "type": "object",
      "properties": {
        "table_name": {
          "type": "string"
        },
        "table_schema": {
          "type": "string"
        },
        "table_alias": {
          "type": "string"
        }
      }
    },
    "protoTemplateArg": {
      "type": "object",
      "properties": {
        "int_val": {
          "type": "string",
          "format": "int64",
          "title": "覆盖 int32"
        },
        "double_val": {
          "type": "number",
          "format": "double",
          "title": "覆盖 float"
        },
        "str_val": {
          "type": "string"
        },
        "bool_val": {
          "type": "boolean"
        },
        "val_items": {
          "$ref": "#/definitions/protoBasicDataArr"
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    }
  }
}
